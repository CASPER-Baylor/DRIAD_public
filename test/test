# copy the latest version of the .exe
#cp ../source-code/IonWake_EXE ./
cp ../IonWake_EXE ./
# Deletes all of the output and error files before
# submiting the job to the GPU nodes. 

# remove all data files
rm ./Data/* &> /dev/null

# remove all output and error files
rm qr-iw.o* &> /dev/null
rm qr-iw.e* &> /dev/null

# submit the job to the gpu nodes
qsub -N qr-iw que-ion-wake -q gpu
